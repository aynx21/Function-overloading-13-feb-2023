#include <iostream>
using namespace std;

int add(int a, int b){
    return a + b ;
}

double add (double a, double b) {
    return a * b;
}
int main() {
    
    int x, y, z;
    double a, b, c;
    
    cout << " Enter your number you want to sum :_" ;
    cin >> x ;
    cout << " Enter the second number you want to sum :_" ;
    cin >> y ;
    z = add(x, y);
    c = add(a, b);
    
   cout << " Int sum is: " << z << endl ;
   cout << " Double sum is : " << c << endl;
